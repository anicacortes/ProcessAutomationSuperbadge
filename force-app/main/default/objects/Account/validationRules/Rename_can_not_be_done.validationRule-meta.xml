<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rename_can_not_be_done</fullName>
    <active>true</active>
    <errorConditionFormula>ISCHANGED(Name) &amp;&amp; (ISPICKVAL(Type, &apos;Customer - Direct&apos;) || ISPICKVAL(Type, &apos;Customer - Channel&apos;))</errorConditionFormula>
    <errorMessage>Account can not be renamed</errorMessage>
</ValidationRule>
